<style>
.apexcharts-text tspan {
    font-family: 'Oblivian Text' !important;
}
</style>
<!--**********************************
        Content body start
***********************************-->
<div class="content-body">
    <!-- row -->
    <div class="container-fluid">
        <div class="row">

            {{!-- Empresas Asignadas --}}
            <div class="col-xl-6 col-lg-6">
                <div class="card">
                    <div class="card-header pb-0 border-0">
                        <span class="p-3 mr-3 rounded bg-secondary" style="background: #FED061 !important;">
                            <svg width="30" height="30" viewBox="0 0 30 30" fill="none"
                                xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
                                <rect width="30" height="30" fill="url(#pattern0)" />
                                <defs>
                                    <pattern id="pattern0" patternContentUnits="objectBoundingBox" width="1" height="1">
                                        <use xlink:href="#image0_234_6803" transform="scale(0.0104167)" />
                                    </pattern>
                                    <image id="image0_234_6803" width="96" height="96"
                                        xlink:href="data:image/png;base64," />
                                </defs>
                            </svg>
                        </span>
                        <div class="mr-auto pr-3">
                            <h4 class="text-black fs-20">Empresas registradas por mes</h4>
                            <p class="fs-13 mb-0 text-black">Asignadas a ti</p>
                        </div>
                    </div>
                   {{#if datosJson_empresas_asignadass}}
                    <input type="hidden" id="datosJson_empresas_asignadas" value="{{datosJson_empresas_asignadas}}">
                    <div class="card-body pb-0">
                        <div id="chartBar"></div>
                    </div>
                    {{else}}
                    <div class="card-body pb-0" style="text-align: center;">
                        <br>
                        <br>
                        <br>
                        <br>
                        <div>
                            <center><i class="far fa-clipboard" style="font-size: 30px; width: 60px;height: 60px; padding-top: 10px;"></i>
                            </center><br>
                            <p>Aún no hay información para mostrar</p>
                        </div>
                    </div>
                    {{/if}}
                </div>
            </div>

            {{!-- Informes generados --}}
            <div class="col-xl-6 col-lg-6">
                <div class="card">
                    <div class="card-header pb-0 border-0">
                        <span class="p-3 mr-3 rounded bg-warning"  style="background: #50368C !important;">
                            <svg width="24" height="30" viewBox="0 0 24 30" fill="none"
                                xmlns="http://www.w3.org/2000/svg">
                                <path
                                    d="M3 0C1.35 0 0.0150003 1.35 0.0150003 3L0 27C0 28.65 1.335 30 2.985 30H21C22.65 30 24 28.65 24 27V9L15 0H3ZM13.5 10.5V2.25L21.75 10.5H13.5Z"
                                    fill="white" />
                            </svg>
                        </span>
                        <div class="mr-auto pr-3">
                            <h4 class="text-black fs-20">Informes generados</h4>
                            <p class="fs-13 mb-0 text-black">Subidos por mes</p>
                        </div>
                    </div>
                     {{#if datosJson_historialI_consultor}}
                    <input type="hidden" id="datosJson_historialI_consultor" value="{{datosJson_historialI_consultor }}">
                    <div class="card-body pb-0">
                        <div id="chartBar3"></div>
                    </div>
                    {{else}}
                    <div class="card-body pb-0" style="text-align: center;">
                        <br>
                        <br>
                        <br>
                        <br>
                        <div>
                            <center><i class="far fa-clipboard" style="font-size: 30px; width: 60px;height: 60px; padding-top: 10px;"></i>
                            </center><br>
                            <p>Aún no hay información para mostrar</p>
                        </div>
                    </div>
                    {{/if}}
                </div>
            </div>

            {{!-- TABLAS --}}

            {{!-- ÚLTIMAS EMPRESAS ASIGNADAS --}}
            <div class="col-xl-6 col-xxl-6">
                <div class="row">
                    <div class="col-xl-12">
                        <div class="card">
                            <div class="card-header">
                                <h4 class="card-title">Últimas empresas asignadas</h4>
                            </div>
                            <div class="card-body">
                                <div class="table-responsive">
                                    <table class="table table-responsive-md text-center">
                                        <thead>
                                            <tr>
                                                <th>FECHA</th>
                                                <th>EMPRESA</th>
                                                <th>NOMBRE</th>
                                                <th>ACCIÓN</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            {{#unless empresas}}
                                            <tr>
                                                <td colspan="4">Aún no hay empresas registrados.</td>
                                            </tr>
                                            {{/unless}}
                                            {{#each empresas}}
                                            <tr>
                                                <td>{{fecha_creacion}}</td>
                                                <td>{{nombre_empresa}}</td>
                                                <td>{{nombres}} {{apellidos}}</td>
                                                <td>
                                                    <div style="margin: 0 auto">
                                                        <a href="/empresas-asignadas/{{codigo}}" class="btn btn-primary shadow btn-xs sharp mr-1"
                                                            style="background: linear-gradient(189.55deg, #FED061 -131.52%, #812082 -11.9%, #50368C 129.46%);">
                                                            <i class="fa fa-pencil"></i>
                                                        </a>
                                                    </div>
                                                </td>
                                            </tr>
                                            {{/each}}
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            {{!-- ÚTIMOS INFORMES CARGADOS --}}
            <div class="col-xl-6 col-xxl-6">
                <div class="row">
                    <div class="col-xl-12">
                        <div class="card">
                            <div class="card-header">
                                <h4 class="card-title">Últimos informes cargados</h4>
                            </div>
                            <div class="card-body">
                                <div class="table-responsive">
                                    <table class="table table-responsive-md text-center">
                                        <thead>
                                            <tr>
                                                <th>NOMBRE</th>
                                                <th>TIPO</th>
                                                <th>EMPRESA</th>
                                                <th>ACCIÓN</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            {{#if ultimosInformes}}
                                            {{#each ultimosInformes}}
                                            <tr>
                                                <td>{{fecha}}</td>
                                                <td>{{etapa}} de negocio</td>
                                                <td>{{nombre}}</td>
                                                <td>
                                                    <a href="{{url}}"
                                                        style="margin: 0 auto; margin-top: -8px;color: white; background: #FED061; border-color: #FED061"
                                                        target="_blank" class="btn btn-info shadow btn-xs sharp">
                                                        <i class="fa fa-search"></i></a>
                                                </td>
                                            </tr>
                                            {{/each}}
                                            {{else}}
                                            <tr>
                                                <td colspan="4">Aún no hay informes generados.</td>
                                            </tr>
                                            {{/if}}
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <br>

        </div>
    </div>
</div>

<!--**********************************
        Fin Content body end
***********************************-->