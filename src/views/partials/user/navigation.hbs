  <div class="header">
    <div class="header-content">
      <nav class="navbar navbar-expand">
        <div class="collapse navbar-collapse justify-content-between">
          <div class="header-left">
            <div class="dashboard_bar">
              {{#comparar itemActivo '==' 1}}Dashboard{{/comparar}}
              {{#comparar itemActivo '==' 2}}Acuerdo de confidencialidad{{/comparar}}
              {{#comparar itemActivo '==' 3}}Diagnóstico de negocio{{/comparar}}
              {{#comparar itemActivo '==' 4}}Análisis de negocio{{/comparar}}
