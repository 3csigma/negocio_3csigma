{{#if botonesEtapas.dos}}
<td style="border-color: transparent; padding:3px 0px 10px" title="No hay correcciones para este informe">
    {{#if crearCorrenccion}}
    {{#if informeOperaciones}}
    <div data-toggle="modal" data-target="#corregirOpe" class="btn color_primary shadow btn-xs sharp mr-1 text-white"
        style="border-color: white;">
        <i class="fa-solid fa-pen-to-square" style="color: #ffffff;"></i>
    </div>
    <div class="modal fade" id="corregirOpe" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel"
        aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header" style="justify-content: center;">
                    <h5 class="modal-title" id="exampleModalLabel">ANOTACIONES DE INFORME</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <form>
                        <div class="form-group">
                            <textarea class="form-control ckeditor" id="correccionOpe">{{infoOpe.correccion}}</textarea>
                        </div>
                    </form>
                </div>
                <div class="modal-footer" style="justify-content: center;">
                    <button onclick="agregarCorreccion(4)" class="btn color_primary text-white" data-dismiss="modal"
                        aria-label="Close">Enviar anotaciones</button>
                    <button onclick="aprobarCorreccion(4)" class="btn color_tertiary text-black" data-dismiss="modal"
                        aria-label="Close">Aprobar informe</button>
                </div>
            </div>
        </div>
    </div>
    {{else}}
    {{> general/verCorreccion}}
    {{/if}}
    {{else}}
    {{#if informeOperaciones}}
    <div data-toggle="modal" data-target="#verCorrecionOpe"
        class="btn color_primary shadow btn-xs sharp mr-1 text-white" style="border-color: white;">
        <i class="fa-solid fa-pen-to-square" style="color: #ffffff;"></i>
    </div>
    <div class="modal fade" id="verCorrecionOpe" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel"
        aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header" style="justify-content: center;">
                    <h5 class="modal-title" id="exampleModalLabel">ANOTACIONES DE INFORME</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <style>
                        .cke_top {
                            display: none;
                        }
                    </style>
                    <div class="form-group" style="padding: 53px;">
                        <textarea class="form-control" id="viewOpe">{{{infoOpe.correccion}}}</textarea>
                    </div>
                </div>
            </div>
        </div>
    </div>
    {{else}}
    {{> general/verCorreccion}}
    {{/if}}
    {{/if}}
</td>
{{#if consultorDash}}
{{#if infoOpe.ok}}
<td style="border-color: transparent; padding:3px">
    <label for="inputInformeOperaciones" title="Este informe ya fue aprobado">
        <div class="btn btn-primary shadow btn-xs sharp mr-1 text-white"
            style="border-color: white; filter: grayscale(1);pointer-events:none" >
            <i class="fa fa-upload"></i>
        </div>
    </label>
</td>
{{else}}
<td style="border-color: transparent; padding:3px">
    <label for="inputInformeOperaciones">
        <div class="btn color_primary shadow btn-xs sharp mr-1 text-white" style="border-color: white;">
            <i class="fa fa-upload"></i>
        </div>
    </label>
    <input id="inputInformeOperaciones" type="file" style="display:none" name="informeOpe" accept="application/pdf" />
</td>
{{/if}}
{{else}}
<td style="border-color: transparent; padding:3px">
    <label for="inputInformeOperaciones">
        <div class="btn color_primary shadow btn-xs sharp mr-1 text-white" style="border-color: white;">
            <i class="fa fa-upload"></i>
        </div>
    </label>
    <input id="inputInformeOperaciones" type="file" style="display:none" name="informeOpe" accept="application/pdf" />
</td>
{{/if}}
{{else}}
{{#unless info.prod.url}}
<button class="btn btn-primary shadow btn-xs sharp mr-1" style="filter:grayscale(1)"><i
        class="fa fa-search"></i></button>
{{/unless}}
{{/if}}