<link rel="stylesheet" href="../css/editarEmpresa.css">
<style>
    .chosen-container-single .chosen-single span {
        font-size: 11px;
    }
</style>
<!--**********************************
        Content body start
***********************************-->
<div class="content-body">
    <!-- row -->
    <div class="container-fluid">

        {{!-- Header Card --}}
        <div class="col-lg-12">
            <div class="profile card card-body px-3 pt-3 pb-0">
                <div class="profile-head">
                    <div class="profile-info">
                        <div class="profile-photo">
                            {{#if datos.foto}}
                            <img src="{{datos.foto}}" class="img-fluid rounded-circle" style="height: 100px;object-fit: cover;" alt="Foto_Perfil_Empresa">
                            {{else}}
                            <img src="../img/profile_default/user.jpg" class="img-fluid rounded-circle" style="height: 100px;object-fit: cover;" alt="Foto_Perfil_Empresa">
                            {{/if}}
                        </div>
                        <div class="profile-details">
                            <div class="profile-name px-3 pt-2">
                                <h4 class="text-primary mb-0">{{datos.nombre_completo}}</h4>
                                <p id="nombreEmpresa">{{datos.nombre_empresa}}</p>
                            </div>
                            <div class="profile px-2 pt-2">
                                <h4 class="text-muted mb-0">{{#if
                                    empresa.telefono}}{{empresa.telefono}}{{else}}N/A{{/if}}</h4>
                                <p>Teléfono</p>
                            </div>

                        </div>
                    </div>
                </div>
            </div>
        </div>

        {{!-- ./Fin Header Card --}}

        <div class="row"> 

            {{!-- COLUNMA IZQUIERDA RESUMEN --}}
            <div class="col-xl-4">
                <div class="card">
                    <div class="card-body">
                        <div class="profile-statistics">
                            <div class="text-center">
                                <div class="row">
                                    <div class="col">
                                        <h3 class="m-b-0">{{#if
                                            empresa.cantidad_socios}}{{empresa.cantidad_socios}}{{else}}N/A{{/if}}</h3>
                                        <span>Cantidad de Socios</span>
                                    </div>
                                    <div class="col">
                                        <h3 class="m-b-0">{{#if empresa.tiempo_experiencia}}
                                            {{empresa.tiempo_experiencia}}{{else}}N/A{{/if}} </h3><span>Años de
                                            experiencia</span>
                                    </div>
                                    <div class="col">
                                        <h3 class="m-b-0">{{#if empresa.num_empleados}}
                                            {{empresa.num_empleados}}{{else}}N/A{{/if}} </h3><span>Número de
                                            Empleados</span>
                                    </div>
                                </div>

                                <div class="mt-4">
                                    {{!-- <form action="/actualizarEmpresa" method="post"> --}}
                                    {{#if adminDash}}
                                    {{!-- Fila para Selectores Asginar consultor & Estado de la cuenta --}}
                                    <div class="row">
                                        <div class="col-12">
                                            <label>Estado</label>
                                            <select id="estadoAdm" class="mr-sm-2 default-select text-center">
                                                {{#comparar datos.estadoAdm '==' '1'}}
                                                <option value="1" selected>Activa</option>
                                                <option value="0">Bloqueada</option>
                                                {{/comparar}}
                                                {{#comparar datos.estadoAdm '!=' '1'}}
                                                <option value="0" selected>Bloqueada</option>
                                                <option value="1">Activa</option>
                                                {{/comparar}}
                                            </select>
                                        </div>
                                    </div>
                                    <div class="row mt-3">
                                        
                                        <div class="col-5">
                                            <label>Consultor</label>
                                            <select name="id_consultor" id="id_consultor" class="form-control sltBuscar chosen-container chosen-container-single required">
                                                <option value=""></option>
                                                {{#each consultores}}
                                                <option {{#comparar id_consultores '==' idCon}} selected {{/comparar}} value="{{id_consultores}}">
                                                    {{nombres}} {{apellidos}}</option>
                                                {{/each}}
                                            </select>
                                        </div>

                                        <div class="col-5">
                                            <label>Etapa</label>
                                            <select id="etapaConsultor" name="etapaConsultor" class="default-select">
                                                <option value="Diagnóstico" selected>Diagnóstico</option>
                                                <option value="Análisis">Análisis</option>
                                                <option value="Plan_Empresarial">Plan Empresarial</option>
                                                <option value="Plan_Estratégico">Plan Estratégico</option>
                                            </select>
                                        </div>
                                        <div class="col-2 pt-3">
                                            <button type="button" onclick="addConsulEtapa()" class="btn btn-sm btn-primary text-white mt-4" style="background: linear-gradient(180deg, #FED061 -149.33%, #812082 -19.27%, #50368C 158.67%); border-color: transparent"><i class="fa fa-check"></i></button>
                                        </div>

                                        <div class="col-12" id="divTablaConsul" style="display: {{divConsultores}};">
                                            <table id="tblConsulAsignados" class="table text-center">
                                                <thead>
                                                    <tr>
                                                        <th>Etapa</th>
                                                        <th>Consultor</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    {{#each consultores_asignados}}
                                                    <tr id="{{idFila}}">
                                                        <td>{{etapa}}</td>
                                                        <td >{{consultor}}</td>
                                                    </tr>
                                                    {{/each}}
                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                    {{/if}}

                                    {{!-- Boton actualizar empresa --}}
                                    <div class="row">
                                        <input type="hidden" value="{{datos.code}}" name="codigo" id="codigoEmpresa">
                                        {{#if adminDash}}
                                        <div class="col-12">
                                            <div class="form-group">
                                                <button type="button" class="btn btn-primary"
                                                    onclick="guardarConsulEtapas()"
                                                    style="background: linear-gradient(180deg, #FED061 -149.33%, #812082 -19.27%, #50368C 158.67%); margin-top: 1em; width: 100%; color:#fff !important; padding: 10px 15px;">
                                                    <p class="font-w500 m-auto">Guardar cambios</p>
                                                </button>
                                            </div>
                                        </div>
                                        {{/if}}


                                        <div class="col-12 mt-3">
                                            {{!-- Datos personales --}}
                                            <div class="profile-personal-info">
                                                <div class="row mb-2">
                                                    <div class="col-sm-12 col-12">
                                                        <h5 class="f-w-500">Etapa de consultoría :</br><span>{{#if
                                                                datos.etapa}}{{datos.etapa}}{{else}}N/A{{/if}}</span>
                                                        </h5>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        
                                    </div>

                                    {{!-- </form> --}}

